@article {Monteiroe1501822,
	author = {Monteiro, Fanny M. and Bach, Lennart T. and Brownlee, Colin and Bown, Paul and Rickaby, Rosalind E. M. and Poulton, Alex J. and Tyrrell, Toby and Beaufort, Luc and Dutkiewicz, Stephanie and Gibbs, Samantha and Gutowska, Magdalena A. and Lee, Renee and Riebesell, Ulf and Young, Jeremy and Ridgwell, Andy},
	title = {Why marine phytoplankton calcify},
	volume = {2},
