@article {Vos1218632,
	author = {Vos, Melissa and Esposito, Giovanni and Edirisinghe, Janaka N. and Vilain, Sven and Haddad, Dominik M. and Slabbaert, Jan R. and Van Meensel, Stefanie and Schaap, Onno and De Strooper, Bart and Meganathan, R. and Morais, Vanessa A. and Verstreken, Patrik},
	title = {Vitamin K2 Is a Mitochondrial Electron Carrier That Rescues Pink1 Deficiency},
	year = {2012},
	doi = {10.1126/science.1218632},
	publisher = {American Association for the Advancement of Science},
	abstract = {Human UBIAD1 localizes to mitochondria and converts vitamin K1 to vitamin K2. Vitamin K2 is best known as a cofactor in blood coagulation, but in bacteria it is a membrane-bound electron carrier. Whether vitamin K2 exerts a similar carrier function in eukaryotic cells is unknown. We identified Drosophila UBIAD1/Heix as a modifier of pink1, a gene mutated in Parkinson{\textquoteright}s disease that affects mitochondrial function. Here, we found that vitamin K2 was necessary and sufficient to transfer electrons in Drosophila mitochondria. Heix mutants showed severe mitochondrial defects that were rescued by vitamin K2, and, similar to ubiquinone, vitamin K2 transferred electrons in Drosophila mitochondria, resulting in more efficient adenosine triphosphate (ATP) production. Thus, mitochondrial dysfunction was rescued by vitamin K2 that serves as a mitochondrial electron carrier, helping to maintain normal ATP production.},
	issn = {0036-8075},
	URL = {http://science.sciencemag.org/content/early/2012/05/14/science.1218632},
	eprint = {http://science.sciencemag.org/content/early/2012/05/14/science.1218632.full.pdf},
	journal = {Science}
}
