@article{10.1371/journal.pone.0107673,
 author = {Martínez, Ana AND Romero, Yanet AND Castillo, Tania AND Mascaró, Maite AND López-Rull, Isabel AND Simões, Nuno AND Arcega-Cabrera, Flor AND Gaxiola, Gabriela AND Barbosa, Andrés},
 journal = {PLOS ONE},
